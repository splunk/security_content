
{% for story in stories %}
[panel_group://workbench_panel_group_{{ story.lowercase_name}}]
label = {{ story.name }}
description = {{ story.description }}
disabled = 0

{% if story.workbench_panels is defined %}
panels = {{ story.workbench_panels | tojson }}
{% else %}
panels = ["panel://workbench_panel_get_notable_history"]
{% endif %}

{% endfor %}


{% for response_task in response_tasks %}
[panel://workbench_panel_{{ response_task.lowercase_name }}]
label = {{ response_task.name }}
description = {{ response_task.description }}
disabled = 0
tokens = {\
{% for token in response_task.inputs %}
{% if token == 'user' %}
    "user": {\
      "valuePrefix": "\"",\
      "valueSuffix": "\"",\
      "delimiter": " OR {{ token }}=",\
      "valueType": "primitive",\
      "value": "identity",\
      "default": "_1!=1"\
    }{% elif token == 'dest'%}
    "dest": {\
      "valuePrefix": "\"",\
      "valueSuffix": "\"",\
      "delimiter": " OR {{ token }}=",\
      "valueType": "primitive",\
      "value": "asset",\
      "default": "_1!=1"\
    }{% elif token == 'process_name' or token == 'parent_process_name' %}
    "file": {\
      "valuePrefix": "\"",\
      "valueSuffix": "\"",\
      "delimiter": " OR {{ token }}=\"*",\
      "valueType": "primitive",\
      "value": "file",\
      "default": "_1!=1"\
    }{% else %}
    "{{ token }}": {\
      "valuePrefix": "\"",\
      "valueSuffix": "*\"",\
      "delimiter": " OR {{ token }}=\"*",\
      "valueType": "primitive",\
      "value": "file",\
      "default": "_1!=1"\
    }{% endif %}{{ "," if not loop.last }}\
{% endfor %}
}\


{% endfor %}